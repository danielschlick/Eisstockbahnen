@page "/about"
@inject IJSRuntime JSRuntime
    <main>
        <div class="container">
            <div class="col s12 m7">
                <h2 class="header">Kontaktdaten</h2>
                <div class="card horizontal">
                    <div class="card-image">
                        <img class="materialboxed" src="img/1419529931945.jpg">
                    </div>
                    <div class="card-stacked">
                        <div class="card-content">
                            <p>Aga Ratzenböck</p>
                            <p>Au 9</p>
                            <p>4733 Heiligenberg</p>
                            <p>Österreich</p>
                            <br />
                            <p>Telefon: 06608363475</p>
                            <p>E-Mail: agaratzenboeck@gmail.com</p>
                        </div>
                        <div class="card-action">
                            <a class="blue-text text-darken-2" href="http://www.heiligenberg.at/">Gemeinde Heiligenberg</a>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div id="map"></div>
            </div>

            <div>
                <a href="https://www.flaticon.com/de/autoren/nikita-golubev" title="Nikita Golubev">Nikita Golubev</a> from <a href="https://www.flaticon.com/de/" title="Flaticon"> www.flaticon.com</a>
            </div>
        </div>
    </main>



    @code{

        protected override async Task OnAfterRenderAsync(bool firstRender)
        {
            if (firstRender)
            {
                await JSRuntime.InvokeVoidAsync("initialize", null);
                StateHasChanged();
            }

        }
    }